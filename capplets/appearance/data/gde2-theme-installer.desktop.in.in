[Desktop Entry]
_Name=Theme Installer
_Comment=Installs themes packages for various parts of the desktop
Exec=gde2-appearance-properties -i %u
Icon=preferences-desktop-theme
Terminal=false
Type=Application
MimeType=application/x-gde2-theme-package;
StartupNotify=true
Categories=GTK;Settings;
Keywords=gde2-control-center;GDE2;install;desktop;themes;
NoDisplay=true
OnlyShowIn=GDE2;
